<header class="navbar">
  <div class="container">
    <%= link_to "Flight Hack", root_path %>
    <nav>
      <ul class="navbar-nav">
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "Help" %></li>
        <% if logged_in? %>
          <li><%= link_to "Profile", '@current_user' %></li>
          <li><%= link_to "Log out", logout_path, method: "delete" %></li>
      </ul>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
